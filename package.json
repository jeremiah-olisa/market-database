{
  "name": "market-database",
  "version": "1.0.0",
  "description": "Market Database Management System for segmentation, customer analysis, and product delivery",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "migrate": "node migrations/migrate.js",
    "seed": "node seeders/seed.js",
    "cleanup": "node seeders/cleanup.js",
    "start": "node index.js",
    "db:truncate": "node utils/db-management.js truncate",
    "db:drop": "node utils/db-management.js drop",
    "db:refresh": "node utils/db-management.js refresh",
    "test": "npx jest",
    "test:watch": "npx jest --watch",
    "test:coverage": "npx jest --coverage",
    "test:integration": "npx jest --testPathPattern=integration",
    "test:unit": "npx jest --testPathPattern=unit",
    "test:performance": "npx jest --testPathPattern=performance",
    "test:constraints": "npx jest --testPathPattern=constraints",
    "test:indexes": "npx jest --testPathPattern=indexes",
    "test:views": "npx jest --testPathPattern=views"
  },
  "dependencies": {
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "@faker-js/faker": "^8.4.1"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "nodemon": "^3.0.2"
  },
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.11.1"
}
